.map-wrap.active
	#map
		script.
			function initMap() {
				var place = {lat: 43.653226, lng: -79.38318429999998};
				var map = new google.maps.Map(document.getElementById('map'), {zoom: 16, center: place});
				var marker = new google.maps.Marker({position: place, map: map});
			}
